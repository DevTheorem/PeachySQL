{
    "name": "theodorejb/peachy-sql",
    "description": "A small, speedy database abstraction layer for PHP",
    "license": "MIT",
    "keywords": [
        "database",
        "abstraction",
        "mysql",
        "SQL Server",
        "sqlsrv"
    ],
    "authors": [
        {
            "name": "Theodore Brown",
            "email": "theodorejb@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
	"require-dev": {
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.1",
        "ramsey/uuid": "^4.1.1",
        "vimeo/psalm": "^4.5"
    },
    "autoload": {
        "psr-4": {"PeachySQL\\": "lib/"}
    },
    "autoload-dev": {
        "psr-4": {"PeachySQL\\Test\\": "test/src/"}
    }
}
